<script>
window.onscroll=function(){
    if(document.documentElement.scrollTop > 200){
        document.querySelector(".gotop-container")
        .classList.add("show")
    }else{
        document.querySelector(".gotop-container")
        .classList.remove("show")
    }
}


document.querySelector(".gotop-container")
.addEventListener("click",()=>{
    window.scrollTo({
        top:0,
        behavior:"smooth"
    })
})
</script>

